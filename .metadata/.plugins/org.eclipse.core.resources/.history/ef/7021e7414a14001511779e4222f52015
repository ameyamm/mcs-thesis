package com.ameyamm.mcsthesis.model

import java.util.Date
/**
 * @author ameya
 */
class Contact(
    private val id : Long,

    // Contact Details
    // Person
    private val _firstName : Option[String] = None,
    private val _lastName : String = None,
    private val _middleName : String = None,
    private val _honorific : String = None,
    private val _suffix : String = None,
    
    // Organization
    private val _organizationName : String = None,
    private val _organizationName2 : String = None,
    private val _contactType : String = None,

    // Personal details
    private val _dateOfBirth : Date = None,
    private val _deceased : Boolean = false,
    private val _employer : String = None,
    private val _gender : String = None,
    private val _industry : String = None,
    private val _language : String = None,
    private val _occupation : String = None,
    
    // Communication 
    private val _contactMethod : Set[String] = None,
    private val _email : Set[String] = None,
    private val _phoneNumber : String = None,

    // Civic Address
    private val _civicAddressId : Long = -1,
    private val _civicAddrType : String = None,
    private val _civicAddrBuildingNum : String = None, 
    private val _civicAddrApartmentNum : String = None, 
    private val _civicAddrCity : String = None,
    private val _civicAddrCountry : String = None,
    private val _civicAddrLine1 : String = None,
    private val _civicAddrLine2 : String = None,
    private val _civicAddrMeridian : String = None, 
    private val _civicAddrNumSuffix : String = None,
    private val _civicAddrPostalCode : String = None,
    private val _civicAddrProvince : String = None,
    private val _civicAddrRange : String = None,
    private val _civicAddrQuarter : String = None,
    private val _civicAddrReserve : String = None, 
    private val _civicAddrSection : String = None,

    // Electoral details 
    private val _federalElectorId : String = None,
    private val _federalPoll : String = None,
    private val _federalRiding : String = None, 
    private val _federalSeqNum : String = None,

    // Misc Properties
    private val _marks : Int = 0,
    private val _leaning : Char = '\0',
    private val _etagName : String = None,
    private val _etagDesc : String = None,
    private val _etagEnumName : String = None,
    private val _etagEnumDesc : String = None,
    private val _etagString : String = None,
    private val _tagDesc : String = None,
    private val _tagName : String = None,
    private val _tagType : String = None,
    private val _surveyResponse : Map[String, String] = None,
    private val _activityListName : String = None,
    private val _activityListStatus : String = None,
    private val _activityType : String = None,
    private val _activityMethod : String = None,
    private val _activityName : String = None,
    private val _activityNotes : String = None,
    private val _activityPurpose : String = None,
    private val _activityStatus : String = None,
    private val _activityTargetName : String = None,
    private val _activityTargetType : String = None,

    // Other Ids
    private val _mailingAddressId : Long = -1,
    private val _contactPoolId : Long = -1) {
  
}