CREATE TABLE t_analytics_contact
(
    contact_id bigint not null,
campaign_id bigint not null,
region_id bigint not null,
activity_id bigint not null,
first_name text,
middle_name text,
last_name text,
contact_type text,
organization_name text,
date_of_birth text,
deceased boolean,
employer text,
gender text,
industry text,
language text,
occupation text,
contact_methods text,
mark int,
leaning text,
cases_filed int,
cases_closed int,
etags text,
survey_responses text,
tags text,
federal_elector_id bigint,
federal_poll bigint,
federal_riding bigint,
federal_seq_number bigint,
has_email boolean,
townhouse_or_apartment text,
civic_address_type text,
civic_address_city text,
civic_address_province text,
civic_address_country text,
civic_address_postal_code text,
civic_address_street_type text,
civic_address_township text
);

